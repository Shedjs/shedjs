{
    "name": "shedjs",
    "version": "1.0.1",
    "description": "A lightweight, dependency-free JavaScript framework",
    "main": "dist/shedjs.js",
    "module": "dist/shedjs.esm.js",
    "types": "dist/types/index.d.ts",
    "type": "module",
    "files": [
        "dist",
        "assets"
    ],
    "scripts": {
        "build": "rollup -c",
        "types": "tsc --emitDeclarationOnly --declaration --outDir dist/types",
        "docs": "typedoc src --out docs",
        "docs:json": "typedoc src --json docs/jsdoc.json",
        "prepublishOnly": "npm run build && npm run types",
        "build:full": "npm run build && npm run types && npm run docs"
    },
    "keywords": [
        "framework",
        "lightweight",
        "frontend",
        "virtual-dom",
        "spa",
        "javascript",
        "typescript"
    ],
    "author": {
        "name": "Shedjs Team"
    },
    "contributors": [
